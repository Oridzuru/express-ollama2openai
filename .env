# 基础配置
NODE_ENV=production
PORT=20203
CLUSTER_ENABLED=true

# 上游服务
OLLAMA_URL=http://127.0.0.1:20202
TIMEOUT=60000

# 安全配置
API_KEYS=prod_key_1,prod_key_2
RATE_LIMIT_WINDOW=900000  # 15分钟
RATE_LIMIT_MAX=500

# 日志配置
LOG_LEVEL=info
LOG_DIR=/var/log/ollama-proxy

# 缓存配置
CACHE_ENABLED=true
CACHE_TTL=1800
